{
  "name": "curso-react",
  "version": "1.0.0",
  "description": "Aplicacion ejemplo",
  "main": "index.js",
  "scripts": {
    "start": "npm run create-dev-resources && npm-run-all --parallel open:src lint:watch",
    "open:src": "node_modules/.bin/webpack-dev-server --info true",
    "lint": "node_modules/.bin/esw --color src/**",
    "lint:watch": "npm run lint -- --watch",
    "build-web": "./node_modules/.bin/webpack --config webpack.config.js --progress -p",
    "build-web-prod": "npm-run-all create-prod-resources && ./node_modules/.bin/webpack --config webpack.config.prod.js --progress -p",
    "test": "NODE_ENV=test ./node_modules/.bin/_mocha --compilers js:babel-core/register src/test/**/*.test.js",
    "test:watch": "npm run test -- --watch",
    "coverage": "NODE_ENV=test ./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover ./node_modules/.bin/_mocha  -- --compilers js:babel-core/register src/**/*.test.js",
    "doc": "./node_modules/.bin/yuidoc .",
    "run-prod": "npm-run-all build-web-prod && ./node_modules/http-server/bin/http-server dist/",
    "create-dev-resources": "rm -rf public/ && mkdir public && cp static/root/*.* public/ && cp -R static/js public/js && mkdir public/images && cp -R static/images/portfolioimages public/images/portfolioimages",
    "create-prod-resources": "rm -rf dist/ && mkdir dist && cp static/root/*.* dist/ && cp -R static/js dist/js && mkdir dist/images && cp -R static/images/portfolioimages dist/images/portfolioimages"
  },
  "engines": {
    "node": "13.13.0",
    "npm": "6.14.4"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "thesuperjez",
  "license": "EPL-1.0",
  "dependencies": {
    "@babel/preset-env": "^7.9.5",
    "axios": "^0.19.0",
    "cd-magnificpopup": "0.0.2",
    "history": "^4.10.1",
    "immutable": "^4.0.0-rc.12",
    "jquery": "1.12.4",
    "jquery-easing": "0.0.1",
    "jquery-migrate": "1.4.1",
    "jquery-parallax": "^1.0.1",
    "jquery-scrollTo": "0.2.1",
    "jquery-ui": "^1.12.1",
    "jquery.simple-text-rotator": "0.0.1",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-modal": "1.9.7",
    "react-redux": "^5.0.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.8",
    "redux": "^4.0.5",
    "redux-thunk": "^2.2.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.24.0",
    "chai": "^4.2.0",
    "colors": "^1.1.2",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^6.0.0",
    "http-server": "^0.12.3",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^6.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.1.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.1.0",
    "imagemin-webpack": "^5.1.1",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.1.1",
    "npm-run-all": "^4.0.2",
    "style-loader": "^1.1.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "yuidocjs": "^0.10.2"
  }
}
